# ===========================================
# 环境变量配置示例
# ===========================================
# 复制此文件为 .env.development 并填入您的真实配置

# 服务器配置
PORT=5001
NODE_ENV=development

# JWT 配置
# 生成密钥: openssl rand -hex 64
JWT_SECRET=your_jwt_secret_here
JWT_ACCESS_TOKEN_EXPIRES=24h
JWT_REFRESH_TOKEN_DAYS=7

# 管理员账号
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_admin_password

# 日志配置
LOG_LEVEL=debug

# CORS配置
CORS_ORIGIN=http://localhost:5174

# 性能监控
TRACKING_INTERVAL=3600000
TRACKING_CONCURRENT=5
TRACKING_BATCH_SIZE=10

# 开发专用配置
DEBUG_MODE=true
HOT_RELOAD=true

# AI 提供商配置
AI_DEFAULT_PROVIDER=qwen
QWEN_API_KEY=your_api_key
QWEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_MODEL=qwen-plus

# 阿里云 ASR（语音识别）
ALIYUN_ASR_APP_KEY=your_app_key
ALIYUN_ASR_ACCESS_KEY=your_access_key
ALIYUN_ASR_SECRET_KEY=your_secret_key
ALIYUN_ASR_REGION=cn-shanghai
