# ===========================================
# 开发环境配置
# ===========================================

# 服务器配置
PORT=5001
NODE_ENV=development

# JWT 配置
# 请使用 openssl rand -hex 64 生成密钥
JWT_SECRET=your_jwt_secret_here_generate_with_openssl_rand_hex_64
JWT_ACCESS_TOKEN_EXPIRES=24h
JWT_REFRESH_TOKEN_DAYS=7

# 管理员账号
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_admin_password_here

# 日志配置
LOG_LEVEL=debug

# CORS配置
CORS_ORIGIN=http://localhost:5174

# 性能监控
TRACKING_INTERVAL=3600000
TRACKING_CONCURRENT=5
TRACKING_BATCH_SIZE=10

# 开发专用配置
DEBUG_MODE=true
HOT_RELOAD=true

# AI 提供商配置
AI_DEFAULT_PROVIDER=qwen
QWEN_API_KEY=your_qwen_api_key_here
QWEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_MODEL=qwen-plus

# 阿里云 ASR（语音识别）
ALIYUN_ASR_APP_KEY=your_aliyun_asr_app_key
ALIYUN_ASR_ACCESS_KEY=your_aliyun_access_key
ALIYUN_ASR_SECRET_KEY=your_aliyun_secret_key
ALIYUN_ASR_REGION=cn-shanghai
